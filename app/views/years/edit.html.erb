<h1>Editing article</h1>
<%=@year %>

<%= form_for :year, url: year_path(@year), method: :patch do |f| %>

    <% if @year.errors.any? %>
        <div id="error_explanation">
          <h2>
            <%= pluralize(@year.errors.count, "error") %> prohibited
            this article from being saved:
          </h2>
          <ul>
            <% @year.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>

    <p>
      <%= f.label :text %><br>
      <%= f.text_area :text %>
    </p>

    <p>
      <%= f.submit %>
    </p>

<% end %>

<%= link_to 'Back', year_path %>